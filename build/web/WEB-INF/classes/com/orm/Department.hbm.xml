<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.orm.Department" table="department">
        <property name="org_id">
            <column name="org_id" not-null="false"/>
        </property>
        <property name="par_id">
            <column name="par_id" not-null="false"/>
        </property>
        <property name="org_name">
            <column name="org_name" not-null="false"/>
        </property>
        <property name="state">
            <column name="state" not-null="false"/>
        </property>
    </class>
</hibernate-mapping>
